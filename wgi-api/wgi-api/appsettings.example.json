{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "WGIProjectConnectionString": "DATABASE_STRING"
    },
    "Auth0": {
        "Domain": "AUTH0_DOMAIN",
        "Audience": "AUTH0_CLIENT_ID"
    },
    "CORSAllow": "FRONT_END_URL" ,
    "AzureSecret": {
        "Key": "AZURE_SECRET_KEY",
        "ConnectionString": "AZURE_CONNECTION_STRING",
        "BlobContainerName": "AZURE_BLOB_NAME"
    }
}
